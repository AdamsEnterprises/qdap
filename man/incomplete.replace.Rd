## The function is currently defined as
function (text.var, en.dash = FALSE, em.dash = FALSE) 
{
    x <- gsub("[.?!]+\\.[.?!]*|[.?!]*\\.[.?!]+", "|", scrubber(text.var))
    if (en.dash) {
        x <- gsub("[<96>]", "|", x)
    }
    if (em.dash) {
        x <- gsub("[<97>]", "|", x)
    }
    return(x)
  }
}
